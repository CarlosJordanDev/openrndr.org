{
  "name": "react-static-typescript-enabled",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "react-static start",
    "stage": "react-static build --staging",
    "build": "mkdir -p public && yarn data:fetch && react-static build",
    "test": "jest --watch",
    "serve": "serve dist -p 3000",
    "data:fetch": "ts-node --project tsconfig-data.json data/index",
    "format": "prettier './**/*.{js,ts,tsx}' --write",
    "lint": "tslint -p . -c tslint.json",
    "precommit": "pretty-quick --staged"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"]
  },
  "dependencies": {
    "@ibm/type": "^0.5.3",
    "datocms-client": "^0.3.41",
    "install": "^0.10.4",
    "lodash": "^4.17.4",
    "npm": "^5.6.0",
    "nuka-carousel": "^4.0.3",
    "prop-types": "^15.6.1",
    "pure-react-carousel": "^1.12.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-imgix": "^7.1.1",
    "react-player": "^1.4.0",
    "react-router": "^4.2.0",
    "react-static": "^5.5.14",
    "react-transition-group": "^2.3.1",
    "react-visibility-sensor": "^3.11.0"
  },
  "devDependencies": {
    "@types/jest": "^22.1.1",
    "@types/lodash": "^4.14.92",
    "@types/node": "^9.4.7",
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.0.40",
    "@types/react-dom": "^16.0.2",
    "@types/react-hot-loader": "^3.0.5",
    "@types/react-router": "^4.0.16",
    "@types/react-router-dom": "^4.2.6",
    "@types/react-transition-group": "^2.0.8",
    "@types/webpack-env": "^1.13.6",
    "babel-eslint": "^8.2.1",
    "convert-tsconfig-paths-to-webpack-aliases": "^0.9.2",
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-react-tools": "1.x.x",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.4.0",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "husky": "^0.14.3",
    "jest": "^22.1.4",
    "nodemon": "^1.17.3",
    "prettier": "^1.10.2",
    "pretty-quick": "^1.2.2",
    "serve": "^6.4.1",
    "ts-jest": "^22.4.4",
    "ts-loader": "^3.1.1",
    "ts-node": "^4.1.0",
    "ts-transformer-keys": "^0.3.1",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.4.2",
    "tslint-config-prettier": "^1.6.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.6.2"
  }
}
